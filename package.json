{
  "name": "password-strength-calculator",
  "version": "1.0.0",
  "description": "Password strength analysis using combinatorics principles - Product Rule and Character Set Analysis",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "test": "npm run test:lib && npm run test:types",
    "test:lib": "vitest --disable-console-intercept",
    "test:coverage": "vitest --run --coverage --disable-console-intercept",
    "test:types": "tsd"
  },
  "keywords": [
    "password",
    "strength",
    "combinatorics",
    "security",
    "mathematics",
    "product-rule"
  ],
  "author": "Anshum",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/coverage-v8": "^2.1.9",
    "jest": "^29.7.0",
    "rollup": "^4.28.0",
    "rollup-plugin-copy": "^3.5.0",
    "tsd": "^0.31.2",
    "vitest": "^2.1.8"
  },
  "tsd": {
    "directory": "test"
  },
  "dependencies": {
    "escape-string-regexp": "^5.0.0"
  }
